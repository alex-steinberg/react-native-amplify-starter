{
  "name": "react-native-amplify-starter",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "android:dev:clean": "npm run cc && NODE_ENV=development expo run:android",
    "android:test:clean": "npm run cc && NODE_ENV=test expo run:android",
    "android:prod:clean": "npm run cc && NODE_ENV=production expo run:android",
    "android:dev": "eas build --profile development --platform android",
    "android:preview": "eas build --profile preview --platform android",
    "android:apk:local": "eas build --profile apk --platform android --local",
    "prebuild:android": "NODE_ENV=production expo prebuild --platform android",
    "prebuild:ios": "NODE_ENV=production expo prebuild --platform ios",
    "ios:local": "eas build --platform ios --local",
    "ios:dev:clean": "npm run cc && NODE_ENV=development expo run:ios",
    "ios:test:clean": "npm run cc && NODE_ENV=test expo run:ios",
    "ios:prod:clean": "npm run cc && NODE_ENV=production expo run:ios",
    "ios:dev": "NODE_ENV=development expo run:ios",
    "ios:test": "NODE_ENV=test expo run:ios",
    "ios:prod": "NODE_ENV=production expo run:ios",
    "ios:build:prod": "NODE_ENV=production eas build --platform ios",
    "ios:build:preview": "NODE_ENV=production eas build --platform ios",
    "ios:submit": "eas submit --platform ios",
    "web": "expo start --web",
    "cc": "rm -rf $TMPDIR/metro-*",
    "ios": "expo run:ios",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@eva-design/eva": "^2.2.0",
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@shopify/flash-list": "1.4.0",
    "@tanstack/react-query": "^4.29.12",
    "@ui-kitten/components": "^5.3.0",
    "@ui-kitten/eva-icons": "^5.3.0",
    "amazon-cognito-identity-js": "^6.2.0",
    "aws-amplify": "^5.2.2",
    "axios": "^1.4.0",
    "core-js": "^3.30.2",
    "expo": "~48.0.18",
    "expo-font": "^11.1.1",
    "expo-image-picker": "^14.1.1",
    "expo-notifications": "^0.18.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-native": "0.71.8",
    "react-native-animated-loader": "^1.0.0",
    "react-native-animated-pagination-dots": "^0.1.73",
    "react-native-country-codes-picker": "^2.3.1",
    "react-native-dialog": "^9.3.0",
    "react-native-dotenv": "^3.4.8",
    "react-native-get-random-values": "~1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-shared-element": "0.8.8",
    "react-native-svg": "13.4.0",
    "react-native-toast-notifications": "^3.3.1",
    "react-native-url-polyfill": "^1.3.0",
    "rn-card-scanner": "^1.1.1",
    "expo-dev-client": "~2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@react-native-community/cli-debugger-ui": "^11.3.0",
    "happy-dom": "^9.20.3",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  },
  "resolutions": {
    "@babel/core": "^7.20.2"
  },
  "private": true
}
